{
    "openapi": "3.1.0",
    "info": {
        "title": "EWMS - Workflow Management Service",
        "summary": "The external interface for EWMS",
        "description": "The external interface for starting, tracking, and retrieving results from tasks within EWMS",
        "contact": {
            "name": "WIPAC Developers",
            "url": "icecube.wisc.edu",
            "email": "developers@icecube.wisc.edu"
        },
        "license": {
            "name": "MIT License"
        },
        "version": "0.0.0"
    },
    "components": {
        "schemas": {
            "task_directive": {
                "type": "object",
                "properties": {
                    "task_id": {
                        "type": "uuid"
                    },
                    "foo": {
                        "type": "int"
                    },
                    "bar": {
                        "type": "int"
                    }
                },
                "required": [
                    "task_id"
                ]
            },
            "taskforce": {
                "type": "object",
                "properties": {
                    "taskforce_uuid": {
                        "type": "uuid"
                    },
                    "foo": {
                        "type": "int"
                    },
                    "bar": {
                        "type": "int"
                    }
                },
                "required": [
                    "taskforce_uuid"
                ]
            }
        }
    },
    "paths": {
        "/example/{id}": {
            "parameters": [
                {
                    "name": "id",
                    "in": "path",
                    "required": true,
                    "description": "the user identifier, as userId",
                    "schema": {
                        "type": "uuid"
                    }
                }
            ],
            "get": {
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "description": "Updated name of the pet",
                                        "type": "string"
                                    },
                                    "status": {
                                        "description": "Updated status of the pet",
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "status"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "the user being returned",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "uuid": {
                                            "type": "string",
                                            "format": "uuid"
                                        }
                                    }
                                }
                            }
                        },
                        "links": {
                            "address": {
                                "operationId": "getUserAddress",
                                "parameters": {
                                    "userId": "$request.path.id"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/": {
            "parameters": [],
            "get": {
                "responses": {
                    "200": {
                        "description": "an empty response",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/tms/job-event-log/{jel_fpath}": {
            "parameters": [
                {
                    "name": "jel_fpath",
                    "in": "path",
                    "required": true,
                    "description": "the condor job event log filepath on the AP (urllib.parse.quote'd)",
                    "schema": {
                        "type": "string"
                    }
                }
            ],
            "post": {
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "collector": {
                                        "description": "name of the condor collector",
                                        "type": "string"
                                    },
                                    "schedd": {
                                        "description": "name of the condor schedd",
                                        "type": "string"
                                    },
                                    "finished": {
                                        "description": "whether this job event log is finished",
                                        "type": "bool"
                                    }
                                },
                                "required": [
                                    "status",
                                    "schedd",
                                    "finished"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "TODO",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/schema/openapi": {
            "parameters": [],
            "get": {
                "responses": {
                    "200": {
                        "description": "the openapi schema",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {}
                                }
                            }
                        }
                    }
                }
            }
        },
        "/task/directive": {
            "parameters": [],
            "post": {
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/task_directive"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "the matching task directive",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/task_directive"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/task/directive/{task_id}": {
            "parameters": [
                {
                    "name": "task_id",
                    "in": "path",
                    "required": true,
                    "description": "the id of the task directive",
                    "schema": {
                        "type": "uuid"
                    }
                }
            ],
            "get": {
                "responses": {
                    "200": {
                        "description": "the matching task directive",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/task_directive"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/task/directives/find": {
            "parameters": [],
            "post": {
                "responses": {
                    "200": {
                        "description": "TODO",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "task_directives": {
                                            "description": "a list of task directive objects",
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/task_directive"
                                            }
                                        }
                                    },
                                    "required": [
                                        "task_directives"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/tms/taskforce/{taskforce_uuid}": {
            "parameters": [
                {
                    "name": "taskforce_uuid",
                    "in": "path",
                    "required": true,
                    "description": "the taskforce object's uuid",
                    "schema": {
                        "type": "uuid"
                    }
                }
            ],
            "get": {
                "responses": {
                    "200": {
                        "description": "the matching taskforce object",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/taskforce"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/tms/taskforces/find": {
            "parameters": [],
            "post": {
                "responses": {
                    "200": {
                        "description": "TODO",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "taskforces": {
                                            "description": "a list of taskforce objects",
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/taskforce"
                                            }
                                        }
                                    },
                                    "required": [
                                        "taskforces"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
